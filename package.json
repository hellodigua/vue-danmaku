{
  "name": "vue-danmaku",
  "description": "基于Vue.js的弹幕交互组件",
  "version": "1.7.2",
  "author": "hellodigua",
  "private": false,
  "main": "dist/vue-danmaku.umd.min.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "docs": "sh scripts/deploy.sh",
    "release": "sh scripts/release.sh",
    "package": "vue-cli-service build --mode package --target lib --name vue-danmaku src/lib/index.js && rm ./dist/demo.html"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^1.19.1",
    "sass": "^1.56.1",
    "sass-loader": "^8.0.2",
    "stats.js": "^0.17.0",
    "vue-template-compiler": "^2.6.12",
    "webpack-cdn-plugin": "^3.3.1"
  },
  "homepage": "https://github.com/hellodigua/vue-danmaku",
  "keywords": [
    "vue",
    "danmaku",
    "vue-danmaku",
    "barrage",
    "vue-barrage",
    "弹幕",
    "年会"
  ],
  "license": "MIT"
}
